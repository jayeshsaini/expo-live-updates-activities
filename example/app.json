{
  "expo": {
    "name": "expo-live-updates-activities-example",
    "slug": "expo-live-updates-activities-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.liveupdates.example",
      "entitlements": {
      "com.apple.security.application-groups": ["group.com.jayesh.liveactivities"]
      },
      "infoPlist": {
        "NSSupportsLiveActivities": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "expo.modules.liveupdates.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
    "@bacons/apple-targets"
    ]
  }
}